<body>
  <div class="layer"></div>
<!-- ! Body -->
<a class="skip-link sr-only" href="#skip-target">Skip to content</a>
<div class="page-flex">
  <!-- ! Sidebar -->
  <aside class="sidebar" id="header">
   {{>header}}
</aside>
  <div class="main-wrapper">
    <!-- ! Main nav -->
    <nav class="main-nav--bg">
        {{>nav}}
    </nav>
    <!-- ! Main -->
   
<br>
<br>
<br>
<div style="margin-left: 60%;">
<form action="buscar/{{title.id_insumo}}" method="post">
<div class="input-group">

       <input type="text" placeholder="Buscar Id" name="id" style="border-radius:5px; border:none;">
      <input type="submit" class="btn btn-primary" value="Buscar">
   
      </div>
  
</form>
</div>
<a class="btn btn-primary ml-4" href="create" role="button">Nuevo Registro</a>
<a class="btn btn-primary ml-4" href="reporte" role="button" onclick="Reportar()">Reporte</a>
  
  
<br>
<br>
    <table class="table table-hover ml-3">
        <thead>
          <tr>
            <th scope="col">Id</th>
            <th scope="col">Insumo</th>
            <th scope="col">Precio Insumo</th>
            <th scope="col">Cantidad Insumo</th>
            <th scope="col">Estado</th>
            <th scope="col">Acci√≥n</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            {{#each title}}
            <tr id="lista">
            <td>{{id_insumo}}</td>
            <td>{{nombre}}</td>
            <td>{{precio}}</td>
            <td>{{cantidad}}</td>
            {{!-- <td><input type="checkbox" name=""></td> --}}
            <td><div class="switch-button">
              <input type="checkbox" name="switch-button" id="switch-label" onclick="estado()" checked class="switch-button__checkbox" >
              <label for="switch-label" class="switch-button__label"></label>
          </div></td>
            <td>
               <a href="/insumos/update/{{id_insumo}}"> <button type="button" class="btn btn-outline-secondary">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-clockwise" viewBox="0 0 16 16">
    <path fill-rule="evenodd" d="M8 3a5 5 0 1 0 4.546 2.914.5.5 0 0 1 .908-.417A6 6 0 1 1 8 2v1z"></path>
    <path d="M8 4.466V.534a.25.25 0 0 1 .41-.192l2.36 1.966c.12.1.12.284 0 .384L8.41 4.658A.25.25 0 0 1 8 4.466z"></path>
  </svg>
                  </button></a>



                  <a onclick="eliminarVenta({{id_insumo}})">
                  <button type="button" class="btn btn-outline-secondary" name="eliminar">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash" viewBox="0 0 16 16">
    <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5Zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5Zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6Z"></path>
    <path d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1ZM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118ZM2.5 3h11V2h-11v1Z"></path>
  </svg>
                    
                  </button></a>
            </td>
          </tr>
{{/each}}

        </tbody>
      </table>
      <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
 <script src="sweetalert2.min.js"></script>
 <link rel="stylesheet" href="sweetalert2.min.css">
  


   
